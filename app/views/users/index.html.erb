<h1>Список пользователей</h1>

<table>
  <tr>
    <th>Имя</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= link_to 'Войти как', login_user_path(user), :method => :post %></td>
    <td><%= user.name %></td>
    <td><%= link_to 'Переименовать', edit_user_path(user) %></td>
    <td><%#= link_to 'Удалить', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Новый пользователь', new_user_path %>
